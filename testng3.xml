<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<test name="SmokeTesting">
	<!-- Grouping the testcases from different classes with a group name to execute it with a single execution -->
	<groups>
		<run>
			<include name="Smoke"/>
		</run>
	</groups>
		<classes>
		<class name="com.test.testng.CarLoan"/>
		<class name="com.test.testng.Example1"/>
		<class name="com.test.testng.Example2"/>
		</classes>
	</test>
</suite> 
